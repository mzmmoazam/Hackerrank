import sys
n=int(input())
k = []
#global o
for i in range(n+1):
    k.append(input().split())
#print(str(k[n]).strip("['").strip("']"))
for i in range(n):
    #print(k[i][0])
    if str(k[n]).strip("['").strip("']")==k[i][0]:
        sum=0
        for h in range(1,len(k[i])):
            sum=sum+float(k[i][h])
        o=sum/(len(k[i])-1)
        print(format(o,'.2f'))
        exit()
