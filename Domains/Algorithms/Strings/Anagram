for _ in range(int(input())):
    s=input()
    if len(s)%2==1:
        print("-1")
        continue
    c=0
    s1,s2=s[:len(s)//2],list(s[len(s)//2:])
    for i in s1:
        if i in s2:
            s2.remove(i)
            pass
        else:
            c+=1
            
    print(c)
    
   
    
